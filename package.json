    "license": "MIT",
    "scripts": {
        "start": "npm run build && npm run watch",
        "build": "npm run build-ts && npm run apidoc && npm run apidocmd",
        "serve": "node dist/server.js",
        "watch": "node dist/server.js",
        "build-ts": "tsc",
        "watch-ts": "tsc -w",
