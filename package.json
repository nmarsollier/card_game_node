        "@types/jsonwebtoken": "8.5.8",
        "@types/mongodb": "4.0.7",
        "@types/morgan": "1.9.3",
        "@types/node": "17.0.27",
        "@types/passport": "1.0.7",
        "@types/passport-jwt": "3.0.6",
        "@types/uuid": "8.3.4",
        "@typescript-eslint/eslint-plugin": "5.21.0",
        "@typescript-eslint/parser": "5.21.0",
        "apidoc": "0.22.1",
        "apidoc-markdown": "7.1.0",
        "concurrently": "7.1.0",
        "eslint": "8.14.0",
