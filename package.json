        "dotenv": "16.0.0",
        "escape-html": "1.0.3",
        "express": "4.18.0",
        "express-async-errors": "3.1.1",
        "express-session": "^1.17.2",
        "helmet": "5.0.2",
        "ioredis": "5.0.4",
        "jsonwebtoken": "8.5.1",
        "mongoose": "6.3.1",
        "morgan": "1.10.0",
        "node-cache": "5.1.2",
        "passport": "0.5.2",
        "passport-jwt": "4.0.0",
        "uuid": "8.3.2",
        "winston": "3.7.2"
